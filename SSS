---anime soul x private---
local GUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/aaaa"))()

local UI = GUI:CreateWindow("ANIME SOULS X","")

local Home = UI:addPage("AUTO RAID",1,true,6)

Home:addLabel("AUTO FARM RAID")

local isToggleOn = false

Home:addToggle("AUTO RAID", function(value)
    isToggleOn = value
    print("Toggle is now:", isToggleOn)
    
    if isToggleOn then
        -- Start the loop
        while isToggleOn do
                        local args = {
                [1] = {
                    [1] = {
                        [1] = "/3",
                        [2] = "Raid",
                        [3] = "Join"
                    }
                }
            }
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2559.52246, 2841.14478, 7122.82373, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            wait(1.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2559.37354, 2833.64478, 7060.31689, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            wait(1.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2511.32227, 2833.64478, 7114.14844, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            wait(1.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2608.8147, 2833.64478, 7116.77979, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            wait(1.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2560.81763, 2833.64478, 7179.78564, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            wait(1)
            wait()  -- Adjust the wait time as needed
        end
    end
end)

-- Replace "_INTERACTIONS" and "RaidPortal" with the actual names of the folder and model
local folderName = "_INTERACTIONS"
local modelName = "RaidPortal"

-- Get the Workspace and Players service
local workspace = game:GetService("Workspace")
local players = game:GetService("Players")

-- Function to teleport a player to a specific position
local function teleportPlayer(player, position)
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Parent = nil
            character:SetPrimaryPartCFrame(CFrame.new(position))
            humanoid.Parent = character
        end
    end
end

-- Function to find and teleport to the RaidPortal model
local function teleportToRaidPortal(player)
    local interactionsFolder = workspace:FindFirstChild(folderName)

    if interactionsFolder and interactionsFolder:IsA("Folder") then
        local raidPortal = interactionsFolder:FindFirstChild(modelName)

        if raidPortal and raidPortal:IsA("Model") and raidPortal.PrimaryPart then
            local portalPosition = raidPortal:GetPrimaryPartCFrame().Position
            teleportPlayer(player, portalPosition)

            -- Add the script to move to specific locations within the raid
            while player.Character and player.Character:FindFirstChild("HumanoidRootPart") do
                wait(1.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2559.52246, 2841.14478, 7122.82373, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                wait(1.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2559.37354, 2833.64478, 7060.31689, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                wait(1.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2511.32227, 2833.64478, 7114.14844, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                wait(1.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2608.8147, 2833.64478, 7116.77979, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                wait(1.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2560.81763, 2833.64478, 7179.78564, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            end
        else
            print("RaidPortal model not found.")
        end
    else
        print("Interactions folder not found.")
    end
end

-- Toggle variable
local isTeleportToggleOn = false

-- Function to handle toggle changes
local function handleTeleportToggle(value)
    isTeleportToggleOn = value
    print("Teleport Toggle is now:", isTeleportToggleOn)

    if isTeleportToggleOn then
        -- Start the loop when the toggle is on
        while isTeleportToggleOn do
            teleportToRaidPortal(players.LocalPlayer)
            wait()
        end
    end
end

-- Call the handleTeleportToggle function to start the loop
handleTeleportToggle(true)  -- Adjust the argument based on whether you want the toggle to start as on or off

-- Toggle variable
local isToggleOn = false

-- Function to handle toggle changes
local function handleToggle(value)
    isToggleOn = value
    print("Toggle is now:", isToggleOn)

    if isToggleOn then
        -- Start the loop when the toggle is on
        while isToggleOn do
            teleportToRaidPortal(players.LocalPlayer)
            wait()
        end
    end
end

-- Call the handleToggle function to start the loop
handleToggle(true)  -- Adjust the argument based on whether you want the toggle to start as on or off

